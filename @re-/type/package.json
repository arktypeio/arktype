{
    "name": "@re-/type",
    "version": "2.0.10-alpha",
    "description": "One type from editor to runtime ðŸ§­",
    "author": {
        "name": "David Blass",
        "email": "david@redo.dev",
        "url": "https://redo.dev"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/re-do/re-po.git",
        "directory": "@re-/type"
    },
    "type": "module",
    "main": "./dist/cjs/index.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/mjs/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "scripts": {
        "build": "ts-node ../../scripts/build.ts",
        "typecheck": "tsc --noEmit",
        "test": "c8 reassert --cmd mocha",
        "tnt": "reassert --skipTypes --cmd mocha",
        "testBuild": "ts-node ../../scripts/testBuild.ts",
        "bench": "reassert bench",
        "generateBenchData": "ts-node ./scripts/generateBenchData.ts",
        "benchParse": "ts-node ./src/nodes/__tests__/parse.bench.ts",
        "benchBound": "ts-node ./src/nodes/nonTerminal/__tests__/bound.bench.ts",
        "benchCyclic": "ts-node ./src/__tests__/space/cyclic.bench.ts",
        "diag": "tsc --composite false --noEmit --diagnostics --incremental false",
        "diagUp": "pnpm diag > diag.txt"
    },
    "dependencies": {
        "@re-/tools": "workspace:*"
    },
    "devDependencies": {
        "@re-/assert": "workspace:*",
        "@re-/node": "workspace:*",
        "fast-check": "3.1.2"
    }
}
