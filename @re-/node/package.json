{
    "name": "@re-/node",
    "version": "2.0.0",
    "author": "redo.dev",
    "description": "Node-based utilities, scripts, and configs for Redo packages ⚙️",
    "repository": {
        "type": "git",
        "url": "https://github.com/re-do/re-po.git",
        "directory": "pkgs/node"
    },
    "type": "module",
    "main": "out/cjs/index.js",
    "types": "out/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./out/types/index.d.ts",
            "import": "./out/esm/index.js",
            "require": "./out/cjs/index.js"
        }
    },
    "scripts": {
        "build": "node --no-warnings --loader ts-node/esm ./src/cli.ts tsc",
        "test": "jest 2>&1",
        "lint": "eslint ."
    },
    "bin": {
        "re": "./out/cjs/cli.js"
    },
    "dependencies": {
        "execa": "5.0.0",
        "prompts": "2.4.1",
        "get-current-line": "6.6.0",
        "deepmerge": "4.2.2",
        "@microsoft/tsdoc": "0.14.1",
        "@microsoft/tsdoc-config": "0.16.1",
        "@microsoft/api-extractor": "7.22.2",
        "@microsoft/api-extractor-model": "7.17.2",
        "@microsoft/api-documenter": "7.17.10",
        "ts-morph": "14.0.0"
    }
}
