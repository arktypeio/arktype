{
    "name": "@re-do/app",
    "description": "The Redo app",
    "author": "redo.qa",
    "version": "0.0.13",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/redo-qa/redo.git",
        "directory": "pkgs/app"
    },
    "main": "dist/main.js",
    "scripts": {
        "start": "jsrx start",
        "electron": "jsrx electron",
        "build": "jsrx build",
        "publish": "jsrx publish",
        "publishLinux": "jsrx publishLinux",
        "publishMac": "jsrx publishMac",
        "publishWindows": "jsrx publishWindows",
        "buildProd": "jsrx buildProd",
        "jsrxGen": "jsrx jsrxGen"
    },
    "build": {
        "appId": "com.redo.app",
        "productName": "redo",
        "directories": {
            "buildResources": "resources",
            "output": "release"
        },
        "files": [
            "dist"
        ],
        "linux": {
            "executableName": "redo",
            "artifactName": "${productName}-${version}.${ext}"
        },
        "mac": {
            "category": "public.app-category.developer-tools"
        }
    },
    "dependencies": {
        "@apollo/client": "3.3.7",
        "@babel/runtime-corejs3": "7.12.5",
        "@hot-loader/react-dom": "17.0.1",
        "@material-ui/core": "4.11.2",
        "@material-ui/styles": "4.11.2",
        "@medv/finder": "2.0.0",
        "@re-do/components": "0.0.4",
        "@re-do/model": "0.0.1",
        "@re-do/test": "0.0.1",
        "@re-do/utils": "0.0.10",
        "core-js": "3.8.2",
        "dotenv": "8.2.0",
        "electron-updater": "4.3.5",
        "framer-motion": "3.2.1",
        "fs-extra": "9.0.1",
        "graphql": "15.4.0",
        "graphql-tag": "2.11.0",
        "playwright-core": "1.7.1",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-statelessly": "0.0.3",
        "tsconfig-paths": "3.9.0",
        "typeface-ubuntu": "1.1.13"
    },
    "devDependencies": {
        "@re-do/bundle": "0.0.1",
        "@re-do/recommended": "0.0.2",
        "@types/dotenv": "8.2.0",
        "@types/electron-devtools-installer": "2.2.0",
        "@types/fs-extra": "9.0.6",
        "@types/node": "14.14.21",
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0",
        "babel-preset-redo": "0.0.2",
        "jsrx": "0.0.3",
        "electron": "11.2.0",
        "electron-builder": "22.9.1",
        "electron-devtools-installer": "3.1.1",
        "react-hot-loader": "4.13.0",
        "ts-node": "9.1.1",
        "tsconfig-paths-webpack-plugin": "3.3.0",
        "typescript": "4.1.3",
        "webpack": "5.17.0",
        "webpack-cli": "4.4.0",
        "webpack-dev-server": "3.11.2"
    }
}
