{
    "name": "@re-do/app",
    "description": "The Redo app",
    "author": "redo.qa",
    "version": "0.0.13",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/redo-qa/redo.git",
        "directory": "pkgs/app"
    },
    "main": "dist/main.js",
    "scripts": {
        "start": "jsrx start",
        "electron": "jsrx electron",
        "build": "jsrx build",
        "publish": "jsrx publish",
        "publishLinux": "jsrx publishLinux",
        "publishMac": "jsrx publishMac",
        "publishWindows": "jsrx publishWindows",
        "buildProd": "jsrx buildProd",
        "jsrxGen": "jsrx jsrxGen"
    },
    "build": {
        "appId": "com.redo.app",
        "productName": "redo",
        "directories": {
            "buildResources": "resources",
            "output": "release"
        },
        "files": [
            "dist"
        ],
        "linux": {
            "executableName": "redo",
            "artifactName": "${productName}-${version}.${ext}"
        },
        "mac": {
            "category": "public.app-category.developer-tools"
        }
    },
    "dependencies": {
        "@apollo/client": "3.0.0-beta.37",
        "@babel/runtime-corejs3": "7.10.3",
        "@hot-loader/react-dom": "16.13.0",
        "@material-ui/core": "4.11.0",
        "@material-ui/styles": "4.10.0",
        "@medv/finder": "1.1.2",
        "@re-do/components": "0.0.4",
        "@re-do/model": "0.0.1",
        "@re-do/test": "0.0.1",
        "@re-do/utils": "0.0.10",
        "core-js": "3.6.1",
        "dotenv": "8.2.0",
        "electron-updater": "4.3.1",
        "framer-motion": "1.7.0",
        "fs-extra": "8.1.0",
        "graphql": "15.1.0",
        "graphql-tag": "2.10.1",
        "playwright-core": "0.11.1",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-statelessly": "0.0.3",
        "tsconfig-paths": "3.9.0",
        "typeface-ubuntu": "0.0.65"
    },
    "devDependencies": {
        "@babel/core": "7.10.3",
        "@re-do/bundle": "0.0.1",
        "@re-do/recommended": "0.0.2",
        "@types/dotenv": "8.2.0",
        "@types/electron-devtools-installer": "2.2.0",
        "@types/fs-extra": "8.1.0",
        "@types/node": "12.11.1",
        "@types/puppeteer-core": "2.0.0",
        "@types/react": "16.9.42",
        "@types/react-dom": "16.9.4",
        "babel-preset-redo": "0.0.2",
        "jsrx": "0.0.2",
        "electron": "9.1.0",
        "electron-builder": "22.7.0",
        "electron-devtools-installer": "3.1.0",
        "react-hot-loader": "4.12.18",
        "ts-node": "8.10.2",
        "tsconfig-paths-webpack-plugin": "3.2.0",
        "typescript": "3.9.5",
        "webpack": "4.43.0",
        "webpack-cli": "3.3.10",
        "webpack-dev-server": "3.11.0"
    }
}
