{
    "name": "@re-do/website",
    "version": "0.0.1",
    "description": "Redo's website (https://redo.qa)",
    "private": true,
    "scripts": {
        "start": "npm run clean && npm run serveWithBrowser",
        "restart": "npm run clean && npm run serveOnly",
        "build": "npm run clean && NODE_ENV=production webpack",
        "serveWithBrowser": "NODE_ENV=development webpack serve --open",
        "serveOnly": "NODE_ENV=development webpack serve",
        "clean": "rm -rf dist && mkdir dist && cp -r assets dist"
    },
    "dependencies": {
        "@babel/runtime-corejs3": "7.12.5",
        "@material-ui/core": "4.11.2",
        "@material-ui/styles": "4.11.2",
        "@material-ui/lab": "4.0.0-alpha.57",
        "@mdx-js/react": "1.6.22",
        "@re-do/components": "0.0.4",
        "@re-do/utils": "0.0.10",
        "analytics-node": "3.5.0",
        "buffer": "6.0.3",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-ga": "3.3.0",
        "react-router-dom": "5.2.0",
        "react-typist": "2.0.5",
        "framer-motion": "3.2.1",
        "uuid": "8.3.2",
        "validator": "13.5.2"
    },
    "devDependencies": {
        "@re-do/bundle": "0.0.1",
        "@re-do/recommended": "0.0.2",
        "@types/node": "14.14.21",
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0",
        "@types/react-typist": "2.0.1",
        "@types/analytics-node": "3.1.4",
        "@types/uuid": "8.3.0",
        "babel-preset-redo": "0.0.2",
        "ts-node": "9.1.1",
        "typescript": "4.1.3",
        "@types/react-router-dom": "5.1.7",
        "webpack": "5.17.0",
        "webpack-cli": "4.4.0",
        "webpack-dev-server": "3.11.2",
        "react-hot-loader": "4.13.0",
        "@types/validator": "13.1.3"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
