{
    "name": "@re-/tools",
    "version": "1.3.0",
    "author": "redo.dev",
    "description": "Utilities, scripts, and configs for the Redo monorepoðŸ§°",
    "repository": {
        "type": "git",
        "url": "https://github.com/re-do/re-po.git",
        "directory": "pkgs/tools"
    },
    "type": "module",
    "main": "out/cjs/index.cjs",
    "types": "out/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./out/types/index.d.ts",
            "import": "./out/esm/index.js",
            "require": "./out/cjs/index.cjs"
        },
        "./node": {
            "types": "./out/types/node/index.d.ts",
            "import": "./out/esm/node/index.js",
            "require": "./out/cjs/node/index.cjs"
        }
    },
    "scripts": {
        "build": "set TS_NODE_TRANSPILE_ONLY=1&& node -r ./filterWarnings.cjs --loader ts-node/esm ./src/node/reTscCli.ts",
        "runScript": "./runScript.js",
        "reTsc": "./reTsc.js",
        "reTranspile": "./reTranspile.js",
        "test": "jest 2>&1"
    },
    "bin": {
        "runScript": "./runScript.js",
        "reTsc": "./reTsc.js",
        "reTranspile": "./reTranspile.js"
    },
    "dependencies": {
        "moize": "5.4.7",
        "ts-toolbelt": "9.5.10",
        "deepmerge": "4.2.2",
        "execa": "5.0.0",
        "prompts": "2.4.1",
        "ts-node": "10.4.0",
        "tree-kill": "1.2.2",
        "get-current-line": "6.6.0",
        "typescript": "4.5.5"
    },
    "devDependencies": {
        "@re-/assert": "0.1.1",
        "@types/node": "17.0.1",
        "jest": "27.4.5",
        "@types/jest": "27.0.3",
        "ts-jest": "27.1.2",
        "@types/prompts": "2.0.11",
        "@jest/types": "27.4.2",
        "vite": "2.4.4"
    }
}
