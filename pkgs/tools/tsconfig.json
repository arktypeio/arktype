{
    "compilerOptions": {
        "module": "esnext",
        "target": "esnext",
        "moduleResolution": "node",
        "skipLibCheck": true,
        "strict": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "composite": true,
        "jsx": "react",
        "lib": ["ESNext"],
        "typeRoots": ["./deno", "node_modules/@types"],
        "paths": {
            "@deno/std/*.ts": ["./deno/std/*.js"]
        }
    },
    "include": ["*"]
}
