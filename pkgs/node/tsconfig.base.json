{
    "compilerOptions": {
        "module": "esnext",
        "target": "esnext",
        "moduleResolution": "node",
        "inlineSourceMap": true,
        "skipLibCheck": true,
        "strict": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "lib": ["ESNext", "dom", "dom.iterable"]
    }
}
