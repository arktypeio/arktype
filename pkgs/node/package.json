{
    "name": "@re-do/node",
    "version": "0.1.2",
    "author": "redo.qa",
    "description": "NodeJS utilities, build tools, and configs",
    "repository": {
        "type": "git",
        "url": "https://github.com/re-do/redo.git",
        "directory": "pkgs/node"
    },
    "type": "module",
    "main": "out/cjs/index.cjs",
    "types": "out/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./out/esm/index.js",
            "require": "./out/cjs/index.cjs",
            "types": "./out/types/index.d.ts"
        }
    },
    "bin": {
        "runScript": "./runScript.js",
        "redoTsc": "./redoTsc.js"
    },
    "scripts": {
        "build": "set TS_NODE_TRANSPILE_ONLY=1&& node -r ./filterWarnings.cjs --loader ts-node/esm ./src/redoTscCli.ts",
        "runScript": "./runScript.js",
        "redoTsc": "./redoTsc.js",
        "test": "echo \"This package has no tests (but let's be honest it should)\""
    },
    "dependencies": {
        "@re-do/utils": "latest",
        "deepmerge": "4.2.2",
        "execa": "5.0.0",
        "moize": "5.4.7",
        "validator": "13.5.2",
        "prompts": "2.4.1",
        "ts-node": "10.4.0",
        "get-current-line": "6.6.0",
        "tree-kill": "1.2.2",
        "typescript": "4.5.0-beta",
        "@jest/reporters": "27.3.1",
        "vite": "2.4.4",
        "@vitejs/plugin-react-refresh": "1.3.6",
        "process": "0.11.10",
        "ts-toolbelt": "9.5.10",
        "fast-deep-equal": "3.1.3"
    },
    "devDependencies": {
        "@types/node": "15.0.2",
        "@types/prompts": "2.0.11",
        "jest": "27.0.6",
        "@types/jest": "26.0.24",
        "@jest/types": "27.2.5",
        "ts-jest": "27.0.3",
        "tsd": "0.17.0"
    }
}
