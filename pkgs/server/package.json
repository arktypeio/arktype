{
    "name": "@re-do/server",
    "version": "0.0.1",
    "private": true,
    "author": "redo.qa",
    "description": "Redo's server",
    "scripts": {
        "build": "NODE_ENV=development && npm run _build",
        "createDb": "npm run _wipeDbDev && NODE_ENV=development npm run _upDb && ts-node prisma/seed.ts",
        "studio": "NODE_ENV=development && npm run _studio",
        "buildProd": "NODE_ENV=production && npm run _build",
        "upDbProd": "NODE_ENV=production && npm run _upDb",
        "studioProd": "NODE_ENV=production && npm run _studio",
        "start": "NODE_ENV=development && sls offline",
        "deploy": "NODE_ENV=production && npm run buildProd && sls deploy",
        "package": "NODE_ENV=production && sls package",
        "_wipeDbDev": "rm -rf prisma/dev.db && npm run _wipeMigrationsDev",
        "_wipeMigrationsDev": "rm -rf prisma/migrations/*-development && sed -i '/-development/d' prisma/migrations/lift.lock",
        "_build": "npm run _generate && tsc",
        "_upDb": "prisma2 lift save --name $NODE_ENV --create-db && prisma2 lift up",
        "_generate": "prisma2 generate && npm run _generateSchema",
        "_generateSchema": "ts-node --transpile-only src/schema && cp schema.gql ../model",
        "_studio": "prisma2 dev"
    },
    "dependencies": {
        "@babel/runtime-corejs3": "7.6.3",
        "@prisma/photon": "2.0.0-preview017.2",
        "@re-do/bundle": "0.0.1",
        "@re-do/utils": "0.0.1",
        "apollo-server-lambda": "2.9.15",
        "bcrypt": "3.0.6",
        "deepmerge": "4.1.1",
        "dotenv": "8.2.0",
        "graphql": "14.5.8",
        "graphql-tag": "2.10.1",
        "isomorphic-ws": "4.0.1",
        "jsonwebtoken": "8.5.1",
        "nexus": "0.12.0-rc.5",
        "redo-nexus-prisma": "0.6.1-9"
    },
    "devDependencies": {
        "@re-do/recommended": "0.0.1",
        "@types/bcrypt": "3.0.0",
        "@types/dotenv": "6.1.1",
        "@types/fs-extra": "8.0.1",
        "@types/graphql": "14.5.0",
        "@types/jest": "24.0.19",
        "@types/jsonwebtoken": "8.3.5",
        "@types/node": "12.11.1",
        "babel-preset-redo": "0.0.1",
        "execa": "4.0.0",
        "fs-extra": "8.1.0",
        "jest": "24.9.0",
        "prisma2": "2.0.0-preview017.2",
        "serverless": "1.60.1",
        "serverless-dotenv-plugin": "2.1.1",
        "serverless-hooks-plugin": "1.1.0",
        "serverless-offline": "5.12.1",
        "serverless-webpack": "5.3.1",
        "ts-node": "8.5.4",
        "ts-node-dev": "1.0.0-pre.43",
        "typescript": "3.7.4"
    }
}
