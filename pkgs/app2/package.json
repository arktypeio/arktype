{
    "name": "app2",
    "version": "0.0.1",
    "private": true,
    "engines": {
        "node": ">=v14.16",
        "npm": ">=7.7"
    },
    "main": "dist/index.cjs",
    "scripts": {
        "buildEnvTypes": "ts-node -O '{\"module\":\"commonjs\"}' scripts/buildEnvTypes.ts",
        "build": "ts-node -O '{\"module\":\"commonjs\"}' scripts/build.ts",
        "precompile": "cross-env MODE=production npm run build",
        "compile": "electron-builder build --config electron-builder.config.js --dir --config.asar=false",
        "pretest": "cross-env MODE=test npm run build",
        "test": "node tests/app.spec.js",
        "watch": "ts-node -O '{\"module\":\"commonjs\"}' scripts/watch.ts",
        "lint": "eslint . --ext js,ts,vue",
        "pretypecheck": "npm run buildEnvTypes",
        "typecheck-main": "tsc --noEmit -p packages/main/tsconfig.json",
        "typecheck-renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
        "typecheck": "npm run typecheck-main && npm run typecheck-preload && npm run typecheck-renderer"
    },
    "browserslist": [
        "Chrome 89"
    ],
    "simple-git-hooks": {
        "pre-commit": "npx lint-staged",
        "pre-push": "npm run typecheck"
    },
    "lint-staged": {
        "*.{js,ts}": "prettier --write"
    },
    "dependencies": {
        "electron-updater": "4.3.5",
        "@apollo/client": "3.3.7",
        "@babel/runtime-corejs3": "7.13.10",
        "@material-ui/core": "4.11.2",
        "@material-ui/styles": "4.11.2",
        "@medv/finder": "2.0.0",
        "@re-do/components": "0.0.4",
        "@re-do/model": "0.0.1",
        "@re-do/test": "0.0.1",
        "@re-do/utils": "0.0.10",
        "core-js": "3.8.2",
        "dotenv": "9.0.0",
        "framer-motion": "4.1.11",
        "fs-extra": "10.0.0",
        "graphql": "15.5.0",
        "graphql-tag": "2.12.4",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-statelessly": "0.0.3",
        "typeface-ubuntu": "1.1.13",
        "isomorphic-ws": "4.0.1",
        "playwright-core": "1.10.0",
        "electron-redux": "1.5.4",
        "@re-do/node-utils": "0.0.1"
    },
    "devDependencies": {
        "@types/electron-devtools-installer": "2.2.0",
        "cross-env": "7.0.3",
        "electron": "11.4.5",
        "electron-builder": "22.9.1",
        "electron-devtools-installer": "3.2.0",
        "lint-staged": "10.5.4",
        "simple-git-hooks": "2.4.1",
        "spectron": "14.0.0",
        "typescript": "4.2.3",
        "vite": "2.2.4",
        "jsrx": "0.0.3",
        "@types/react": "17.0.5",
        "@types/react-dom": "17.0.3",
        "@types/node": "15.0.2",
        "@types/fs-extra": "9.0.6",
        "@types/dotenv": "8.2.0",
        "@vitejs/plugin-react-refresh": "1.3.3",
        "ts-node": "9.1.1"
    }
}
