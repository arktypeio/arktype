{
    "name": "@re-do/node-utils",
    "version": "0.1.1",
    "author": "redo.qa",
    "description": "Node utilities consumed by Redo",
    "repository": {
        "type": "git",
        "url": "https://github.com/re-do/redo.git",
        "directory": "pkgs/node-utils"
    },
    "type": "module",
    "main": "out/cjs/index.js",
    "types": "out/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./out/esm/index.js",
            "require": "./out/cjs/index.js",
            "types": "./out/types/index.d.ts"
        }
    },
    "bin": {
        "runScript": "./runScript.js",
        "redoTsc": "./redoTsc.js"
    },
    "scripts": {
        "build": "node --loader ts-node/esm ./src/redoTsc.ts",
        "runScript": "./runScript.js",
        "test": "jest"
    },
    "dependencies": {
        "@re-do/utils": "workspace:*",
        "deepmerge": "4.2.2",
        "execa": "5.0.0",
        "moize": "5.4.7",
        "validator": "13.5.2",
        "prompts": "2.4.1",
        "ts-node": "10.4.0",
        "get-current-line": "6.6.0",
        "tree-kill": "1.2.2",
        "typescript": "4.5.0-beta",
        "retypes": "workspace:*",
        "@jest/reporters": "27.3.1",
        "vite": "2.4.4",
        "@vitejs/plugin-react-refresh": "1.3.6"
    },
    "devDependencies": {
        "@types/node": "15.0.2",
        "@types/prompts": "2.0.11",
        "jest": "27.0.6",
        "@types/jest": "26.0.24",
        "@jest/types": "27.2.5",
        "ts-jest": "27.0.3"
    }
}
