{
    "name": "@re-/model",
    "version": "1.11.0",
    "author": "redo.dev",
    "description": "Type-first validation from editor to runtimeðŸ§¬",
    "repository": {
        "type": "git",
        "url": "https://github.com/re-do/re-po.git",
        "directory": "pkgs/model"
    },
    "type": "module",
    "main": "out/cjs/index.cjs",
    "types": "out/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./out/types/index.d.ts",
            "import": "./out/esm/index.js",
            "require": "./out/cjs/index.cjs"
        }
    },
    "//": "2>&1 redirects stderr to stdout to avoid rush test failing CI since Jest always outputs to stderr",
    "scripts": {
        "build": "reTsc",
        "test": "jest 2>&1",
        "unit": "jest --coverage false -t ",
        "snapshotTsPerf": "tsc --extendedDiagnostics --noEmit > tsDiagnostics.txt",
        "generateDocs": "runScript docgen/generateDocs.ts"
    },
    "dependencies": {
        "@re-/tools": "workspace:*"
    },
    "devDependencies": {
        "@re-/assert": "workspace:*",
        "@re-/node": "workspace:*",
        "jest": "27.4.5",
        "@types/jest": "27.0.3",
        "ts-jest": "27.1.2",
        "@types/node": "17.0.1",
        "typescript": "4.6.3",
        "@microsoft/tsdoc": "0.14.1",
        "@microsoft/tsdoc-config": "0.16.1",
        "@microsoft/api-extractor": "7.22.2",
        "@microsoft/api-extractor-model": "7.17.2",
        "@microsoft/api-documenter": "7.17.10",
        "prettier": "2.6.2",
        "@types/prettier": "2.6.0",
        "ts-node": "10.4.0"
    }
