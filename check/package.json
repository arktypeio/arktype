{
    "name": "@arktype/check",
    "version": "0.0.1",
    "description": "Assertion framework for isomorphic TS/JS",
    "author": {
        "name": "David Blass",
        "email": "david@arktype.io",
        "url": "https://arktype.io"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/arktypeio/arktype.git",
        "directory": "check"
    },
    "type": "module",
    "main": "./dist/cjs/index.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/mjs/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "scripts": {
        "build": "pnpm ts-node ../meta/scripts/build.ts",
        "test": "pnpm ts-node ./src/cli.ts --cmd mocha",
        "reassert": "./cli.cjs"
    },
    "bin": {
        "reassert": "./cli.cjs"
    },
    "dependencies": {
        "@arktype/node": "workspace:*",
        "@arktype/tools": "workspace:*",
        "arktype": "workspace:*",
        "ts-morph": "16.0.0",
        "source-map-js": "1.0.2",
        "convert-source-map": "1.8.0"
    }
}
