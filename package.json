{
    "name": "@re-/po",
    "private": true,
    "packageManager": "pnpm@7.1.2",
    "type": "module",
    "scripts": {
        "build": "pnpm -r build",
        "lint": "eslint .",
        "test": "pnpm -r test",
        "bench": "pnpm -r bench",
        "pr-checks": "pnpm build && pnpm lint && pnpm test && pnpm bench",
        "changeset": "changeset",
        "preinstall": "npx only-allow pnpm",
        "prepare": "husky install",
        "pre-commit": "pretty-quick --staged",
        "find-unused": "ts-node ./scripts/findUnused.ts",
        "ci:version": "changeset version && ts-node ./scripts/addVersionSuffixes.ts",
        "ci:publish": "changeset publish"
    },
    "devDependencies": {
        "@changesets/cli": "2.22.0",
        "@swc/cli": "0.1.57",
        "@swc/core": "1.2.197",
        "@types/mocha": "9.1.1",
        "@types/node": "17.0.41",
        "@typescript-eslint/eslint-plugin": "5.27.1",
        "@typescript-eslint/parser": "5.27.1",
        "commitizen": "4.2.4",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "8.17.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-define-config": "1.5.1",
        "eslint-import-resolver-typescript": "2.7.1",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-unicorn": "42.0.0",
        "husky": "8.0.1",
        "mocha": "10.0.0",
        "prettier": "2.7.0",
        "pretty-quick": "3.1.3",
        "ts-morph": "15.1.0",
        "ts-node": "10.8.1",
        "typescript": "4.7.3"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}
