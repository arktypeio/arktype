{
    "name": "redo.dev",
    "version": "0.0.0",
    "author": "redo.dev",
    "private": true,
    "scripts": {
        "docusaurus": "docusaurus",
        "start": "docusaurus start",
        "in": "pnpm install --ignore-workspace",
        "build": "pnpm ts-node ./build.mts",
        "dry-run": "pnpm ts-node ./build.mts --dry-run",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve --dir ../docs",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "typecheck": "tsc",
        "test": "echo No tests for docs!"
    },
    "dependencies": {
        "@docusaurus/core": "2.0.0-beta.21",
        "@docusaurus/plugin-content-docs": "2.0.0-beta.21",
        "@docusaurus/preset-classic": "2.0.0-beta.21",
        "@docusaurus/theme-common": "2.0.0-beta.21",
        "@emotion/react": "11.9.3",
        "@emotion/styled": "11.9.3",
        "@mdx-js/react": "1.6.22",
        "@mui/icons-material": "5.8.4",
        "@mui/material": "5.8.5",
        "framer-motion": "6.3.13",
        "prism-react-renderer": "1.3.3",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-helmet-async": "1.3.0"
    },
    "devDependencies": {
        "@re-/node": "link:../@re-/node",
        "@docusaurus/module-type-aliases": "2.0.0-beta.21",
        "@docusaurus/types": "2.0.0-beta.21",
        "@types/react": "17.0.37",
        "@types/react-dom": "17.0.11",
        "esbuild-loader": "2.19.0",
        "ts-node": "10.8.1",
        "typescript": "4.7.4",
        "@types/node": "18.0.0"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "webpack",
                "@babel/core"
            ]
        }
    }
}
