{
    "name": "redo.dev",
    "version": "0.0.4",
    "author": "redo.dev",
    "private": true,
    "scripts": {
        "docusaurus": "docusaurus",
        "start": "docusaurus start",
        "build": "pnpm install --ignore-workspace && pnpm ts-node ./build.mts",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve --dir dist",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "typecheck": "tsc",
        "test": "echo No tests for docs!",
        "update-apis": "pnpm ts-node ./updateApis.mts",
        "set-current-model-version": "docusaurus docs:version:model ",
        "set-current-state-version": "docusaurus docs:version:state ",
        "set-current-test-version": "docusaurus docs:version:test ",
        "build-pages": "npm run build && rm -rf ../../docs && mkdir ../../docs && cp -r dist/* ../../docs"
    },
    "dependencies": {
        "@docusaurus/core": "2.0.0-beta.21",
        "@docusaurus/plugin-content-docs": "2.0.0-beta.21",
        "@docusaurus/preset-classic": "2.0.0-beta.21",
        "@docusaurus/theme-common": "2.0.0-beta.21",
        "@emotion/react": "11.8.1",
        "@emotion/styled": "11.8.1",
        "@mdx-js/react": "1.6.22",
        "@mui/icons-material": "5.6.2",
        "@mui/material": "5.6.2",
        "clsx": "1.1.1",
        "framer-motion": "6.3.11",
        "prism-react-renderer": "1.3.1",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-helmet-async": "1.3.0"
    },
    "devDependencies": {
        "@re-/node": "link:../@re-/node",
        "@re-/doc": "link:../@re-/doc",
        "@docusaurus/module-type-aliases": "2.0.0-beta.21",
        "@docusaurus/types": "2.0.0-beta.21",
        "@tsconfig/docusaurus": "1.0.5",
        "@types/react": "17.0.37",
        "@types/react-dom": "17.0.11",
        "esbuild-loader": "2.18.0",
        "ts-node": "10.8.1",
        "typescript": "4.7.4",
        "@types/node": "18.0.0"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "webpack",
                "@babel/core"
            ]
        }
    }
}
