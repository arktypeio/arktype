{
	"name": "@arktype/attest",
	"version": "0.6.7",
	"author": {
		"name": "David Blass",
		"email": "david@arktype.io",
		"url": "https://arktype.io"
	},
	"type": "module",
	"main": "./out/main.js",
	"types": "./out/main.d.ts",
	"exports": {
		".": {
			"types": "./out/main.d.ts",
			"default": "./out/main.js"
		},
		"./internal/*": {
			"default": "./out/*"
		}
	},
	"files": [
		"out",
		"!__tests__",
		"**/*.ts"
	],
	"bin": {
		"attest": "./out/cli/cli.js"
	},
	"scripts": {
		"build": "tsx ../repo/build.ts",
		"test": "tsx ../repo/testPackage.ts",
		"bunTest": "bun test --preload ../repo/bunTestSetup.ts"
	},
	"dependencies": {
		"arktype": "2.0.0.dev-5",
		"@arktype/fs": "0.0.15",
		"@arktype/util": "0.0.32",
		"@typescript/vfs": "1.5.0",
		"@typescript/analyze-trace": "0.10.1"
	},
	"devDependencies": {
		"typescript": "5.4.2",
		"typescript-next": "npm:typescript@next"
	},
	"peerDependencies": {
		"typescript": "*"
	}
}
