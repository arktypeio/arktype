{
	"name": "@arktype/attest",
	"version": "0.1.1",
	"author": {
		"name": "David Blass",
		"email": "david@arktype.io",
		"url": "https://arktype.io"
	},
	"type": "module",
	"main": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"exports": {
		".": {
			"arktype-repo": "./main.js",
			"types": "./dist/main.d.ts",
			"import": "./dist/main.js",
			"default": "./dist/main.cjs"
		},
		"./internal/*": {
			"arktype-repo": "./*",
			"import": "./dist/*"
		}
	},
	"files": [
		"dist",
		"!__tests__",
		"**/*.ts"
	],
	"scripts": {
		"build": "tsup --config ../repo/tsup.config.ts",
		"test": "ts ../repo/testPackage.ts",
		"bunTest": "bun test --preload ../repo/bunTestSetup.ts"
	},
	"dependencies": {
		"arktype": "workspace:*",
		"@arktype/fs": "workspace:*",
		"@arktype/util": "workspace:*",
		"@typescript/vfs": "1.5.0",
		"typescript": "5.3.2"
	},
	"peerDependencies": {
		"typescript": "*"
	}
}
