{
	"name": "@ark/attest",
	"version": "0.14.0",
	"author": {
		"name": "David Blass",
		"email": "david@arktype.io",
		"url": "https://arktype.io"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/arktypeio/arktype.git",
		"directory": "ark/attest"
	},
	"type": "module",
	"main": "./out/index.js",
	"types": "./out/index.d.ts",
	"exports": {
		".": {
			"ark-ts": "./index.ts",
			"default": "./out/index.js"
		},
		"./internal/*": {
			"ark-ts": "./*",
			"default": "./out/*"
		}
	},
	"files": [
		"out"
	],
	"bin": {
		"attest": "out/cli/cli.js"
	},
	"scripts": {
		"build": "ts ../repo/build.ts",
		"test": "ts ../repo/testPackage.ts"
	},
	"dependencies": {
		"arktype": "workspace:*",
		"@ark/fs": "workspace:*",
		"@ark/util": "workspace:*",
		"@typescript/vfs": "1.6.0",
		"@typescript/analyze-trace": "0.10.1",
		"prettier": "3.3.3",
		"@prettier/sync": "0.5.2"
	},
	"devDependencies": {
		"typescript": "catalog:"
	},
	"peerDependencies": {
		"typescript": "*"
	},
	"publishConfig": {
		"access": "public"
	}
}
