{
	"name": "@arktype/attest",
	"version": "0.2.0",
	"author": {
		"name": "David Blass",
		"email": "david@arktype.io",
		"url": "https://arktype.io"
	},
	"type": "module",
	"main": "./dist/main.cjs",
	"types": "./dist/main.d.cts",
	"exports": {
		".": {
			"arktype-repo": "./main.js",
			"import": {
				"types": "./dist/main.d.ts",
				"default": "./dist/main.js"
			},
			"default": {
				"types": "./dist/main.d.cts",
				"default": "./dist/main.cjs"
			}
		}
	},
	"files": [
		"dist",
		"!__tests__",
		"**/*.ts"
	],
	"scripts": {
		"build": "tsup --config ../repo/tsup.config.ts",
		"test": "ts ../repo/testPackage.ts",
		"bunTest": "bun test --preload ../repo/bunTestSetup.ts"
	},
	"dependencies": {
		"arktype": "workspace:*",
		"@arktype/fs": "workspace:*",
		"@arktype/util": "workspace:*",
		"@typescript/vfs": "1.5.0"
	},
	"devDependencies": {
		"typescript": "5.3.2"
	},
	"peerDependencies": {
		"typescript": "*"
	}
}
