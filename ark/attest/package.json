{
	"name": "@arktype/attest",
	"version": "0.6.6",
	"author": {
		"name": "David Blass",
		"email": "david@arktype.io",
		"url": "https://arktype.io"
	},
	"type": "module",
	"main": "./out/main.js",
	"types": "./out/main.d.ts",
	"exports": {
		".": {
			"types": "./out/main.d.ts",
			"default": "./out/main.js"
		},
		"./internal/*": {
			"default": "./out/*"
		}
	},
	"files": ["out", "!__tests__", "**/*.ts"],
	"bin": {
		"attest": "./out/cli/cli.js"
	},
	"scripts": {
		"build": "tsx ../repo/build.ts",
		"test": "tsx ../repo/testPackage.ts",
		"bunTest": "bun test --preload ../repo/bunTestSetup.ts"
	},
	"dependencies": {
		"arktype": "workspace:*",
		"@arktype/fs": "workspace:*",
		"@arktype/util": "workspace:*",
		"@typescript/vfs": "1.5.0",
		"@typescript/analyze-trace": "0.10.1"
	},
	"devDependencies": {
		"typescript": "5.4.2",
		"typescript-next": "npm:typescript@next"
	},
	"peerDependencies": {
		"typescript": "*"
	}
}
