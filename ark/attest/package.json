{
	"name": "@arktype/attest",
	"version": "0.1.1",
	"author": {
		"name": "David Blass",
		"email": "david@arktype.io",
		"url": "https://arktype.io"
	},
	"type": "module",
	"exports": {
		".": {
			"arktype-repo": "./main.js",
			"types": "./dist/main.d.ts",
			"default": "./dist/main.js"
		},
		"./internal/*": {
			"arktype-repo": "./*",
			"types": "./dist/*",
			"default": "./dist/*"
		}
	},
	"files": [
		"dist",
		"**/*.ts",
		"!**/*.tsBuildInfo",
		"!__tests__"
	],
	"scripts": {
		"build": "tsc --build ./tsconfig.build.json",
		"test": "ts ../repo/testPackage.ts",
		"bunTest": "bun test --preload ../repo/bunTestSetup.ts"
	},
	"dependencies": {
		"arktype": "workspace:*",
		"@arktype/fs": "workspace:*",
		"@arktype/util": "workspace:*",
		"@typescript/vfs": "1.5.0"
	},
	"peerDependencies": {
		"typescript": "*"
	}
}
