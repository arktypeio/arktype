{
	"name": "@arktype/attest",
	"version": "0.5.2",
	"author": {
		"name": "David Blass",
		"email": "david@arktype.io",
		"url": "https://arktype.io"
	},
	"type": "module",
	"main": "./out/main.js",
	"types": "./out/main.d.ts",
	"exports": {
		".": {
			"types": "./out/main.d.ts",
			"default": "./out/main.js"
		},
		"./internal/*": {
			"default": "./out/*"
		}
	},
	"files": [
		"out",
		"!__tests__",
		"**/*.ts"
	],
	"bin": {
		"attestPrecache": "./attestPrecache.js"
	},
	"scripts": {
		"build": "tsx ../repo/build.ts",
		"test": "tsx ../repo/testPackage.ts",
		"bunTest": "bun test --preload ../repo/bunTestSetup.ts"
	},
	"dependencies": {
		"arktype": "latest",
		"@arktype/fs": "0.0.11",
		"@arktype/util": "0.0.19",
		"@typescript/vfs": "1.5.0"
	},
	"devDependencies": {
		"typescript": "5.3.3",
		"typescript-next": "npm:typescript@next"
	},
	"peerDependencies": {
		"typescript": "*"
	}
}
