{
	"name": "@arktype/fs",
	"version": "0.0.4",
	"author": {
		"name": "David Blass",
		"email": "david@arktype.io",
		"url": "https://arktype.io"
	},
	"type": "module",
	"exports": {
		".": {
			"arktype-repo": "./main.js",
			"types": "./dist/main.d.ts",
			"default": "./dist/main.js"
		},
		"./internal/*": {
			"arktype-repo": "./*",
			"types": "./dist/*",
			"default": "./dist/*"
		}
	},
	"files": [
		"dist",
		"**/*.ts",
		"!**/*.tsBuildInfo",
		"!__tests__"
	],
	"scripts": {
		"build": "tsc --build"
	}
}
