{
	"name": "@ark/fs",
	"version": "0.52.0",
	"license": "MIT",
	"author": {
		"name": "David Blass",
		"email": "david@arktype.io",
		"url": "https://arktype.io"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/arktypeio/arktype.git",
		"directory": "ark/fs"
	},
	"type": "module",
	"main": "./out/index.js",
	"types": "./out/index.d.ts",
	"exports": {
		".": {
			"ark-ts": "./index.ts",
			"default": "./out/index.js"
		},
		"./internal/*.ts": {
			"ark-ts": "./*.ts",
			"default": "./out/*.js"
		},
		"./internal/*.js": {
			"ark-ts": "./*.ts",
			"default": "./out/*.js"
		}
	},
	"scripts": {
		"build": "ts ../repo/build.ts"
	},
	"files": [
		"out"
	],
	"publishConfig": {
		"access": "public"
	}
}
