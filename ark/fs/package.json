{
	"name": "@arktype/fs",
	"version": "0.0.4",
	"author": {
		"name": "David Blass",
		"email": "david@arktype.io",
		"url": "https://arktype.io"
	},
	"type": "module",
	"main": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"exports": {
		".": {
			"arktype-repo": "./main.js",
			"types": "./dist/main.d.ts",
			"import": "./dist/main.js",
			"default": "./dist/main.cjs"
		},
		"./internal/*": {
			"arktype-repo": "./*",
			"import": "./dist/*"
		}
	},
	"scripts": {
		"build": "tsup --config ../repo/tsup.config.ts"
	},
	"files": [
		"dist",
		"!__tests__",
		"**/*.ts"
	]
}
