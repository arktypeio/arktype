{
	"compilerOptions": {
		"module": "NodeNext",
		"target": "ES2023",
		"moduleResolution": "NodeNext",
		"lib": ["ESNext"],
		"noEmit": true,
		"skipLibCheck": true,
		"strict": true,
		"noUncheckedSideEffectImports": true,
		"declaration": true,
		"verbatimModuleSyntax": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"exactOptionalPropertyTypes": true,
		"forceConsistentCasingInFileNames": true
	}
}
