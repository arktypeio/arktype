{
	"name": "@ark/util",
	"version": "0.2.1",
	"author": {
		"name": "David Blass",
		"email": "david@arktype.io",
		"url": "https://arktype.io"
	},
	"type": "module",
	"main": "./out/index.js",
	"types": "./out/index.d.ts",
	"exports": {
		".": {
			"ark-ts": "./index.ts",
			"default": "./out/index.js"
		},
		"./internal/*": {
			"ark-ts": "./*",
			"default": "./out/*"
		},
		"./tsconfig.base.json": "./tsconfig.base.json"
	},
	"files": [
		"out",
		"tsconfig.base.json"
	],
	"scripts": {
		"build": "tsx --conditions ark-ts ../repo/build.ts",
		"test": "tsx --conditions ark-ts ../repo/testPackage.ts"
	},
	"publishConfig": {
		"access": "public"
	}
}
