{
	"name": "@arktype/util",
	"version": "0.0.5",
	"author": {
		"name": "David Blass",
		"email": "david@arktype.io",
		"url": "https://arktype.io"
	},
	"type": "module",
	"main": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"exports": {
		".": {
			"arktype-repo": "./main.js",
			"types": "./dist/main.d.ts",
			"default": "./dist/main.js"
		},
		"./internal/*": {
			"arktype-repo": "./*",
			"types": "./dist/*",
			"default": "./dist/*"
		}
	},
	"files": [
		"dist",
		"**/*.ts",
		"!**/*.tsBuildInfo",
		"!__tests__"
	],
	"scripts": {
		"build": "tsc --build ./tsconfig.build.json",
		"test": "ts ../repo/testPackage.ts"
	}
}
