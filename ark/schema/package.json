{
	"name": "@arktype/schema",
	"version": "0.0.2",
	"license": "MIT",
	"author": {
		"name": "David Blass",
		"email": "david@arktype.io",
		"url": "https://arktype.io"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/arktypeio/arktype.git"
	},
	"type": "module",
	"main": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"exports": {
		".": {
			"arktype-repo": "./main.js",
			"types": "./dist/main.d.ts",
			"default": "./dist/main.js"
		},
		"./internal/*": {
			"arktype-repo": "./*",
			"types": "./dist/*",
			"default": "./dist/*"
		}
	},
	"files": [
		"dist",
		"**/*.ts",
		"!**/*.tsBuildInfo",
		"!__tests__"
	],
	"scripts": {
		"build": "tsc --build ./tsconfig.build.json",
		"bench": "ts ./__tests__/parse.bench.ts",
		"test": "ts ../repo/testPackage.ts"
	},
	"dependencies": {
		"@arktype/util": "workspace:*"
	}
}
