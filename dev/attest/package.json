{
    "name": "@arktype/attest",
    "version": "0.0.0",
    "private": true,
    "author": {
        "name": "David Blass",
        "email": "david@arktype.io",
        "url": "https://arktype.io"
    },
    "type": "module",
    "main": "./dist/main.js",
    "types": "./dist/main.d.ts",
    "exports": {
        ".": "./dist/main.js"
    },
    "files": [
        "dist",
        "!*.tsbuildinfo"
    ],
    "dependencies": {
        "ts-morph": "18.0.0"
    },
    "devDependencies": {
        "ts-node": "10.9.1",
        "mocha": "10.2.0",
        "typescript": "4.9.5"
    },
    "mocha": {
        "spec": [
            "./test/*.test.*"
        ],
        "ignore": "**/node_modules/*",
        "node-option": [
            "loader=ts-node/esm",
            "no-warnings=ExperimentalWarning"
        ],
        "require": "../configs/mocha.fixtures.ts"
    },
    "scripts": {
        "build": "ts-node ../scripts/build.ts",
        "test": "pnpm mocha"
    }
}
