{
    "name": "arktype-attest",
    "version": "0.0.0",
    "author": {
        "name": "David Blass",
        "email": "david@arktype.io",
        "url": "https://arktype.io"
    },
    "description": "",
    "type": "module",
    "devDependencies": {
        "@oclif/core": "2.8.0",
        "arktype": "1.0.10-alpha",
        "c8": "7.13.0",
        "get-current-line": "6.6.0",
        "mocha": "10.2.0",
        "oclif": "3.7.3",
        "shx": "^0.3.3",
        "ts-morph": "18.0.0",
        "ts-node": "10.9.1",
        "typescript": "5.0.3"
    },
    "files": [
        "/src",
        "/dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/types/main.d.ts",
            "import": "./dist/mjs/main.js",
            "require": "./dist/cjs/main.js"
        }
    },
    "mocha": {
        "spec": [],
        "ignore": "**/node_modules/*",
        "node-option": [
            "loader=ts-node/esm",
            "no-warnings=ExperimentalWarning"
        ]
    },
    "main": "./dist/cjs/main.js",
    "types": "./dist/types/main.d.ts",
    "bin": {
        "attest": "./dist/cjs/commands/test.js"
    },
    "keywords": [
        "typescript"
    ],
    "oclif": {
        "commands": "./dist/cjs/commands/",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " ",
        "topics": {
            "hello": {
                "description": "Say hello to the world and others"
            },
            "test": {
                "description": "Run some tests or something"
            }
        }
    },
    "scripts": {
        "build": "ts-node ../scripts/build.ts",
        "lint": "eslint . --ext .ts --config .eslintrc",
        "test": "mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif readme && git add README.md"
    },
    "dependencies": {
        "@types/node": "^18.15.11"
    }
}
