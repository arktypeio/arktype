{
    "name": "@arktype/test",
    "private": true,
    "version": "0.0.0",
    "description": "Assertion framework for isomorphic TS/JS",
    "author": {
        "name": "David Blass",
        "email": "david@arktype.io",
        "url": "https://arktype.io"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/arktypeio/arktype.git",
        "directory": "dev/test"
    },
    "type": "module",
    "main": "./dist/cjs/api.js",
    "types": "./dist/types/api.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/api.d.ts",
            "import": "./dist/mjs/api.js",
            "require": "./dist/cjs/api.js"
        }
    },
    "scripts": {
        "build": "pnpm ts-node ../scripts/build.ts",
        "typecheck": "tsc --noEmit",
        "test": "pnpm ts-node ./src/cli.ts --cmd mocha"
    },
    "bin": {
        "attest": "./cli.js"
    },
    "dependencies": {
        "arktype": "workspace:*",
        "@arktype/runtime": "workspace:*",
        "ts-morph": "16.0.0"
    }
}
