{
    "name": "@arktype/runtime",
    "version": "0.0.0",
    "private": true,
    "description": "Node-based utilities for ArkType devtools",
    "author": {
        "name": "David Blass",
        "email": "david@arktype.io",
        "url": "https://arktype.io"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/arktypeio/arktype.git",
        "directory": "dev/runtime"
    },
    "type": "module",
    "main": "./dist/cjs/api.js",
    "types": "./dist/types/api.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/api.d.ts",
            "import": "./dist/mjs/api.js",
            "require": "./dist/cjs/api.js"
        }
    },
    "scripts": {
        "build": "pnpm ts-node ../scripts/build.ts",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "get-current-line": "6.6.0",
        "arktype": "workspace:*"
    }
}
